{
  "name": "@everpack/cli",
  "version": "0.1.4",
  "description": "XKit - 开发者工具 CLI",
  "type": "module",
  "bin": {
    "xkit": "./dist/index.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild",
    "start": "node dist/index.mjs",
    "xkit": "node dist/index.mjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "prerelease": "pnpm test && pnpm build",
    "release": "pnpm prerelease && bumpp && pnpm publish --no-git-checks"
  },
  "dependencies": {
    "cac": "^6.7.14",
    "fs-extra": "^11.2.0",
    "giget": "^1.2.3",
    "picocolors": "^1.1.1",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.0.0",
    "@types/prompts": "^2.4.9",
    "bumpp": "^10.4.1",
    "typescript": "^5.6.0",
    "unbuild": "^2.0.0",
    "vitest": "^2.1.0"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/everpack/cli"
  },
  "keywords": [
    "cli",
    "scaffold",
    "template",
    "xkit"
  ],
  "license": "MIT"
}
